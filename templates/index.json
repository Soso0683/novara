            "liquid": "{% assign now_s = 'now' | date: '%s' %}\n{% assign tl2id = 'tl2-' | append: now_s %}\n<div id=\"{{ tl2id }}\" class=\"tl2it\" data-ship=\"2\">\n  <div class=\"tl2it__line\" aria-hidden=\"true\"></div>\n  <!-- Fase 1 -->\n  <div class=\"tl2it__step\">\n    <div class=\"tl2it__dot\">\n      <img class=\"tl2it__icon\"\n           src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_054315_ca3eda7c-c44f-4188-a770-740919ab85d4.jpg?v=1754797492\"\n           alt=\"Pedido realizado\">\n    </div>\n    <div class=\"tl2it__date\" data-role=\"order\">–</div>\n    <div class=\"tl2it__label\">Pedido realizado</div>\n  </div>\n  <!-- Fase 2 -->\n  <div class=\"tl2it__step\">\n    <div class=\"tl2it__dot\">\n      <img class=\"tl2it__icon\"\n           src=\"https://cdn.shopify.com/s/files/1/0937/6546/8502/files/Capture_d_ecran_2025-08-10_054424.jpg?v=1754797492\"\n           alt=\"Enviado\">\n    </div>\n    <div class=\"tl2it__date\" data-role=\"ship\">–</div>\n    <div class=\"tl2it__label\">Enviado</div>\n  </div>\n</div>\n<style>\n  /* Scope por ID para garantizar la visualización */\n  #{{ tl2id }}.tl2it{\n    --dot: 56px; /* diámetro de los círculos */\n    --icon: 26px; /* tamaño de los iconos */\n    --line: 3px; /* grosor de la barra */\n    --edge: 18px; /* margen reducido en los extremos de la barra */\n    --fs-date: 13px;\n    --fs-label: 12px;\n    --black: #111;\n    color:#111; font-family:inherit;\n    display:grid; grid-template-columns:repeat(2, 1fr);\n    align-items:start; justify-items:center;\n    gap:0; position:relative;\n    margin: 6px 0;\n  }\n  /* Barra detrás de los círculos, acortada con --edge */\n  #{{ tl2id }} .tl2it__line{\n    position:absolute;\n    left:calc(var(--dot)/2 + var(--edge));\n    right:calc(var(--dot)/2 + var(--edge));\n    top:calc(var(--dot)/2 - var(--line)/2);\n    height:var(--line); background:var(--black);\n    z-index:0;\n  }\n  #{{ tl2id }} .tl2it__step{ display:flex; flex-direction:column; align-items:center; text-align:center; }\n  #{{ tl2id }} .tl2it__dot{\n    width:var(--dot); height:var(--dot); border-radius:50%;\n    background:var(--black); display:grid; place-items:center; overflow:hidden;\n    position:relative; z-index:1;\n  }\n  #{{ tl2id }} .tl2it__icon{ width:var(--icon); height:var(--icon); object-fit:contain; display:block; }\n  #{{ tl2id }} .tl2it__date{ margin-top:10px; font-weight:700; font-size:var(--fs-date); }\n  #{{ tl2id }} .tl2it__label{ font-size:var(--fs-label); color:#5e5e5e; margin-top:2px; }\n  @media (max-width:600px){\n    #{{ tl2id }}.tl2it{ --dot:52px; --icon:22px; --fs-date:12px; --fs-label:11px; --edge:14px; }\n  }\n</style>\n<script>\n(function(){\n  const root = document.getElementById('{{ tl2id }}'); if(!root) return;\n  const ship = +root.dataset.ship || 2; // +2 días por defecto\n  const today = new Date();\n  const addDays = (d,n)=>{ const x=new Date(d); x.setDate(x.getDate()+n); return x; };\n\n  // Abreviaciones ES: ene, feb, mar, abr, may, jun, jul, ago, sep, oct, nov, dic\n  const monthAbbrevLen = [3,3,3,3,3,3,3,3,3,3,3,3];\n\n  function fmtAbbrev(date){\n    const d = new Date(date);\n    const day = new Intl.DateTimeFormat('es-ES', { day:'numeric' }).format(d);\n    const monthLong = new Intl.DateTimeFormat('es-ES', { month:'long' }).format(d);\n    const len = monthAbbrevLen[d.getMonth()];\n    const abbr = monthLong.substring(0, len);\n    return `${day} ${abbr}`;\n  }\n\n  root.querySelector('[data-role=\"order\"]').textContent = fmtAbbrev(today);\n  root.querySelector('[data-role=\"ship\"]').textContent = fmtAbbrev(addDays(today, ship));\n})();\n</script>"
          }
        }
