root.querySelectorAll('.rev__dot');\n  const count = slides.length;\n  let i = 0, w = root.querySelector('.rev__viewport').clientWidth;\n  function go(n, animate=true){\n    i = (n + count) % count;\n    track.style.transition = animate ? 'transform .35s ease' : 'none';\n    track.style.transform = `translateX(${-i * w}px)`;\n    dots.forEach((d,idx)=>d.classList.toggle('is-active', idx===i));\n  }\n  dots.forEach(d => d.addEventListener('click', () => go(+d.dataset.i)));\n  new ResizeObserver(()=>{ w = root.querySelector('.rev__viewport').clientWidth; go(i,false); }).observe(root);\n  // Swipe\n  let startX = 0, delta = 0, grabbing = false;\n  const vp = root.querySelector('.rev__viewport');\n  const start = x => { grabbing = true; startX = x; delta = 0; track.style.transition='none'; };\n  const move = x => { if(!grabbing) return; delta = x - startX; track.style.transform = `translateX(${-(i*w) + delta}px)`; };\n  const end = () => { if(!grabbing) return; grabbing=false; const thr = Math.max(40, w*0.12); if(delta > thr) go(i-1); else if(delta < -thr) go(i+1); else go(i); };\n  vp.addEventListener('pointerdown', e=>{vp.setPointerCapture(e.pointerId); start(e.clientX);});\n  vp.addEventListener('pointermove', e=>move(e.clientX));\n  vp.addEventListener('pointerup', end);\n  vp.addEventListener('pointercancel', end);\n  vp.addEventListener('touchstart', e=>start(e.touches[0].clientX), {passive:true});\n  vp.addEventListener('touchmove', e=>move(e.touches[0].clientX), {passive:true});\n  vp.addEventListener('touchend', end);\n  go(0,false);\n})();\n</script>"
